<div id="<%= dom_id podcast %>">
  <p>
    <strong>Title:</strong>
    <%= podcast.title %>
  </p>

  <p>
    <strong>Link:</strong>
    <%= podcast.link %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= podcast.description %>
  </p>

  <p>
    <strong>Subtitle:</strong>
    <%= podcast.subtitle %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= podcast.author %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= podcast.image %>
  </p>

</div>
